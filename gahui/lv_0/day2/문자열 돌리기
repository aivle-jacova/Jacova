import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
        
        for(int i=0; i<a.length(); i++){
                System.out.println(a.charAt(i));  // charAt(i) : 문자열의 i번째 글자를 하나씩 출력
        }
        
    }
}