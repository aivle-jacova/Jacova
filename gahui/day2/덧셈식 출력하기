import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int sum = a+b;
        
        for(int i=0; i<=100; i++){
            if(1<=a && b<=100);
        }

        System.out.println(a + " + " + b + " = " + sum);
    }
}